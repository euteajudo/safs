# Configurações do Banco de Dados PostgreSQL
DATABASE_URL=postgresql://safs_user:safs_password@localhost:5432/safs_db

# Configurações do PostgreSQL (para Docker)
POSTGRES_DB=safs_db
POSTGRES_USER=safs_user
POSTGRES_PASSWORD=safs_password

# Configurações do PgAdmin (para Docker)
PGADMIN_DEFAULT_EMAIL=admin@safs.local
PGADMIN_DEFAULT_PASSWORD=admin123

# Configurações da API FastAPI
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=True

# Configurações de Segurança JWT
SECRET_KEY=your-super-secret-key-change-in-production-minimum-32-characters
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Configurações CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:3002,http://127.0.0.1:3000